<h1>Your Cart</h1>
{{#if logged_in}}
    {{#if cartItem.length}}
        <ul>
            {{#each cartItems}}
                <li>
                    {{this.cartItem.name}} 
                    <!-- Add other item details here -->
                </li>
            {{/each}}
        </ul>
    {{else}}
        <p>Your cart is empty.</p>
    {{/if}}
{{else}}
    <p>This is coming from the cart.handlebars template</p>
{{/if}}